{
  "name": "texme",
  "version": "1.0.1-dev",
  "description": "Self-rendering Markdown + LaTeX documents",
  "main": "texme.js",
  "files": [
    "texme.js",
    "texme.min.js"
  ],
  "keywords": [
    "markdown",
    "commonmark",
    "tex",
    "latex",
    "browser",
    "node"
  ],
  "homepage": "https://github.com/susam/texme",
  "bugs": "https://github.com/susam/texme/issues",
  "license": "MIT",
  "author": "Susam Pal <susam@susam.in> (http://susam.in/)",
  "repository": "github:susam/texme",
  "dependencies": {
    "marked": "4.0.12"
  },
  "devDependencies": {
    "coveralls": "latest",
    "jsdoc": "latest",
    "jsdom": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "standard": "latest",
    "uglify-js": "latest"
  },
  "scripts": {
    "test": "standard && mocha",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "min": "uglifyjs texme.js --compress --mangle --output texme.min.js",
    "docs": "jsdoc -c jsdoc.json"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "global": [
      "MathJax"
    ]
  }
}
